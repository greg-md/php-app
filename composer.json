{
  "name": "greg-md/php-app",
  "type": "app",
  "description": "Greg PHP Application",
  "keywords": [ "greg", "php-app" ],
  "homepage": "https://github.com/greg-md/php-app",
  "license": "MIT",
  "authors": [
    { "name": "Grigorii Duca", "email": "contact@greg.md", "homepage": "http://greg.md/" }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^5.6 || ^7.0",
    "greg-md/php-support": "dev-master",
    "greg-md/php-framework": "dev-master",
    "greg-md/php-cache": "dev-master",
    "greg-md/php-mailer": "dev-master",
    "greg-md/php-orm": "dev-master",
    "greg-md/php-static-image": "dev-master",
    "greg-md/php-view": "dev-master",
    "maximebf/debugbar": "^1.0.0",
    "symfony/var-dumper": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "php init.php"
    ]
  }
}
